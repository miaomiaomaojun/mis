	<script type="text/javascript">
		$(function() {
			$(".dingdan_right div.holder").jPages({
				containerID: "itemContainer", //需要被分页的容器
				last: "尾页",
				previous: " ", //前进后退 内容字
				next: " ",
				startPage: 1, //起始显示的页码
				perPage:7, //每页的内容数量
				midRange: 1, //分页器中间页数
				startRange: 1, //开头页数
				endRange: 1 //结尾页数	
			});
		});

		//待评价编辑状态显示出来
		var manyidu,weidao,huanjing,fuwu;
		$(".allshow .manyidu i").click(function() {
			$(".allshow .manyidu i").removeClass("active");
			$(".detailed").removeClass("active");
			$(this).addClass("active").parents("li").children(".detailed").addClass("active");
			$(".detailed:hidden").find(".tupian").find("div").remove();
			manyidu=$(this).index();
//			console.log($(manyidu))
		});
		$(".allshow .bianji").click(function() {
			var hidden = $(this).parents("li").children(".detailed");
			if(hidden.hasClass("active")) {
				return
			} else {
				$(".allshow .manyidu i").removeClass("active");
				$(".detailed").removeClass("active");
				hidden.addClass("active");
				$(".detailed:hidden").find(".tupian").find("div").remove();
			}
		})
		//星星效果
		$(".detailed .stars dd i").click(function() {
			$(this).parent().children("i").removeClass("active");
			for(var i = 0; i <= $(this).index(); i++) {
				$(this).parent().children("i").eq(i).addClass("active");
			}
			mingzi=$(this).parents("dl")[0].className
			if(mingzi=="weidao"){
				weidao=$(this).index()+1;
			}else if(mingzi=="huanjing"){
				huanjing=$(this).index()+1;
			}else if(mingzi=="fuwu"){
				fuwu=$(this).index()+1;
			}
//			console.log(weidao);
//			console.log(huanjing);
//			console.log(fuwu);
		});
	
		
		//输入评论
		$(".commentes_wap .bottom textarea").keyup(function(){
			var length=500-$(this).val().length;
			var i=$(this).parents(".bottom").find("i");
			i.html(length);
			if(length>0){
				i.removeClass("active");
			}else{
				i.addClass("active");
			}
		})
	
	
		//图片上传前的预览
		var leng=0;
		$(".tupian span").click(function() {
			var _this = $(this);
			//没有传图片删除
			_this.parents(".tupian").find("input").each(function(a, b) {
				if($(b).val()) {
					
				}else {
					$(this).parent("div").remove();
				}
			});
					
			var length = _this.parents(".tupian").find("img").length;
			if(length < 5) {
				_this.before('<div><img src="" alt="" id="ImgPr' + leng + '" /><input type="file" accept="image/gif,image/jpg,image/jpeg,image/bmp,image/png" id="chuan' + leng + '"/><i> </i></div>');
				_this.prev().children("i").click(function(){
					$(this).parent("div").remove();
				});
				$(function() {
					$("#chuan" + leng).uploadPreview({ Img: "ImgPr" + leng, Width: 50, Height: 50, ImgType: ["gif", "jpeg", "jpg", "bmp", "png"] });
				});
				$("#chuan" + leng).click();
				leng++;
			};
		})
		
		$(".detailed .tijiao button").click(function(){			
			var pinglun=$(this).parents(".detailed").find("textarea").val();
			var inputs=$(this).parents(".detailed").find(".tupian").find("input");
			var arr=[];
			inputs.each(function(a,b){
				arr.push($(b).val());
			})
//			console.log(manyidu);
//			console.log(weidao);
//			console.log(huanjing);
//			console.log(fuwu);
//			console.log(pinglun);
//			console.log(arr);
		})
	</script>
