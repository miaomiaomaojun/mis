	<script type="text/javascript">
		$(function() {
			$(".dingdan_right div.holder").jPages({
				containerID: "itemContainer", //需要被分页的容器
				last: "尾页",
				previous: " ", //前进后退 内容字
				next: " ",
				startPage: 1, //起始显示的页码
				perPage:4, //每页的内容数量
				midRange: 1, //分页器中间页数
				startRange: 1, //开头页数
				endRange: 1 //结尾页数	
			});
		});
		//小图片点击，切换大图
		$(".content .imgs").on("click","img",function(){
			var src=$(this).attr("src")		//获取小图片地址
			var $detailed=$(this).parents('.list_main').find(".detailed");
			$(".detailed").removeClass("active"); //隐藏其他大图
			$(".content .imgs img").removeClass("active");//清除其他小图的外框
			$(this).addClass('active');						//这个小图加外框
			$detailed.addClass("active").find("img").attr("src",src);		//这个大图显示并设置地址
			
			var height=$detailed.children("img").height();
			$detailed.children("div").css("height",height);	
		})
		
		//图片左右鼠标移入区域  出现左右切换图片的提示
		$(".detailed div").mouseenter(function(){
			var leng=$(this).parents('.list_main').find(".content .imgs img").length;
			console.log(11);
			if(leng>1){
				$(this).children("span").addClass("active");
			}
		})
		$(".detailed div").mouseleave(function(){
			$(this).children("span").removeClass("active");
		})
		
		//点击左右切换大图片
		$(".detailed div").on("click",function(){
			var $imgs=$(this).parents('.list_main').find(".content .imgs");
			var leng=$imgs.children('img').length-1;
			var index=$imgs.children("img.active").attr("index");
			if($(this).hasClass("detailed_left")){
				index--;
				if(index<0){
					index=leng;
				}
			}else{
				index++;
				if(index>leng){
					index=0
				}
			}
			$imgs.children("img").removeClass("active");
			var src=$imgs.children("[index="+index+"]").addClass("active").attr("src");		
			$(this).parents(".detailed").children('img').attr("src",src);	
			var height=$(this).parents(".detailed").children("img").height();
			console.log(height);
			$(this).parents(".detailed").children("div").css("height",height);
		})
		
		//删除订单
		$(".instructions .del").click(function(){
			$(this).parents(".list_main").remove();
		//这里需要和后台交互
			location.replace(location.href);
		})
	</script>
