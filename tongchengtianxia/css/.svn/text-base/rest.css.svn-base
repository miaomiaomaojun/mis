@charset "utf-8";
body,img,hr,h1,h2,h3,h4,h5,h6,ul,ol,li,p,form,input,label,select,button,dl,dt,dd{margin: 0;padding: 0;}
body{font:14px/1.5 "微软雅黑","黑体","宋体";width:100%;}
img{ border:none;vertical-align:middle;}
a{text-decoration: none;}
/*a,a:hover{transition:background 0.5s linear;-webkit-transition:background 0.5s linear;-moz-transition:background 0.5s linear;-o-transition:background 0.5s linear;}*/
li{list-style: none;}
em, i{font-style: normal;}
b{font-weight: normal;}
select,textarea{outline:none;border:none;background:none;}
textarea{resize:none;}
p{line-height:22px;}
input{outline: none;}
.clear{ clear:both; height:0px; width:100%; overflow:hidden;}


:-moz-placeholder { /* Mozilla Firefox 4 to 18 */
    color: #ccc;  
}
::-moz-placeholder { /* Mozilla Firefox 19+ */
    color: #ccc;
}
input:-ms-input-placeholder,
textarea:-ms-input-placeholder {
    color: #ccc;
}
input::-webkit-input-placeholder,
textarea::-webkit-input-placeholder {
    color: #ccc;
}

.zhezhao{
	width: 100%;
	height: 100%;
	position: fixed;
	left: 0;
	top: 0;
	opacity: 0.3;
	filter: alpha(opacity=30);
	z-index: 10;
	background: #000;
	display: none;
}

.zhezhao_bor{
	position: fixed;
	left: 50%;
	top: 50%;
	margin: -153px 0 0 -153px;
	width:330px;
	height:330px;
	padding: 6px;
	border: 2px solid #fff;
	z-index: 11;
	display: none;
}
.zhezhao_bor .zhezhao_in{
	width: 100%;
	height: 100%;
	background: #fff;
	overflow: hidden;
	text-align: center;
}
.zhezhao_bor .zhezhao_in img{
	width: 124px;
	height: 124px;
	position: absolute;
	left: 50%;
	top: 50%;
	margin: -94px 0 0 -64px;
}
.zhezhao_in p{
	text-align: center;
	margin-top:230px ;
}
.zhezhao_in a{
	text-align: center;
	display: inline-block;
	width: 100px;
	height: 32px;
	border: 1px solid #eb6100;
	background: url(/Myweb/img/btn2.png);
	cursor: pointer;
	line-height: 32px;
	color: #fff;
	margin-top: 10px;
}
