/*分页器样式*/
.details_left .holder{
	clear: both;
	display: block;
	width: 100%;
	height: 40px;
	float: inherit;
	text-align: center;
	margin: 0;
}
.details_left .holder a{
	font-size: 12px;
	border: 1px solid #eeeeef;
	color: #999;
}
.details_left .holder a:hover {
	background-color: #fff;
	border-color: #F79F1E;
	color: #F79F1E;
}
.details_left .holder a.jp-current,
.details_left .holder a.jp-current:hover{ 
	color: #FFF;
	font-weight: bold;
	border-color: #F79F1E;
	background: #F79F1E;
}
 .holder a.jp-disabled,
.details_left .holder a.jp-disabled:hover {
	color: #bbb;
}
 .holder a.jp-current,
 .holder a.jp-current:hover,
 .holder a.jp-disabled,
 .holder a.jp-disabled:hover {
	cursor: default; 
}

