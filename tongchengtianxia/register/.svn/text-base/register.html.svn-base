		<meta name="description" content="详细介绍">				
		<link rel="shortcut icon" href="/Myweb/img/favicon.ico"/>
		<link href="/Myweb/css/rest.css" rel="stylesheet" type="text/css" />
		<link href="/Myweb/css/register.css" rel="stylesheet" type="text/css" />		
	</head>
	<body>
		<div class="zhezhao"></div>		
		<div class="zhezhao_bor">
			<div class="zhezhao_in">
				<img src="/Myweb/img/biglogo.png" alt="" />
				<p>小同正在努力，请稍后……</p>
			</div>			
		</div>
		<div class="inner">
			<div class="wrap">
				<div class="center">
					<div class="c_header">
						<h1 class="logo">
							<a href="/Myweb/index.html">
								<img src="/Myweb/img/logo.png" alt="同城天下" title="同城天下" />
							</a>						
						</h1>
						<div class="login">
							<span>我已注册，马上登陆</span>
							<a href="login.html">登陆</a>
						</div>
					</div>
					<!--注册信息填写-->
					<div class="c_main">
						<div class="forms">
							<label for="">用户名</label>
							<input type="text" id="username" placeholder="请输入您的手机号"/>
							<div class="f_tip">
								<i id="f_tip_i1"></i>
								<span id="f_tip_span1"></span>
							</div>							
						</div>
						<div class="forms yanzheng">
							<label for="">验证码</label>
							<input type="text" id="f_iptyanzheng" disabled="disabled"/>
							<span class="yanzhengma">获取验证码</span>
							<div class="f_tip">
								<i id="f_tip_i2"></i>
								<span id="f_tip_span2"></span>
							</div>
						</div>
						<div class="forms mima">
							<label for="">密码</label>
							<input type="password" id="password" placeholder="请输入5-15个字符密码"/>
							<div class="f_tip">
								<i id="f_tip_i3"></i>
								<span id="f_tip_span3"></span>
							</div>
							<div class="qiangdu">
								<label for=""></label>
								<div id="q_xianshi">
									<span class="span1">弱</span>
									<span class="mg1 span2">中</span>
									<span class="span3">强</span>
								</div>	
							</div>
						</div >
						<div class="forms">
							<label for="">确认密码</label>
							<input type="password" id="password2"/>
							<div class="f_tip">
								<i id="f_tip_i4"></i>
								<span id="f_tip_span4"></span>
							</div>
						</div>
						<div class="forms">	
							<label for=""></label>
							<button id="register">同意协议并注册</button>
						</div>
						<div class="forms">
							<label for=""></label>
							<a href="">《服务协议》</a>
						</div>
					</div>	
				</div>		
			</div>
			<div class="footer">
